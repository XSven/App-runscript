#!perl

use strict;
use warnings;

use Cwd            qw( abs_path );
use File::Basename qw( dirname );
use File::Spec     qw();

my $install_base;
BEGIN { $install_base = dirname dirname abs_path __FILE__ }

use lib File::Spec->catdir( $install_base, qw( lib perl5 ) );

use App::runscript ();

App::runscript::main @ARGV;
